<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="el.dv.dvproperties.feature.home.view.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_newPropertyFragment"
            app:destination="@id/newPropertyFragment" />
        <action
            android:id="@+id/action_homeFragment_to_propertyDetailsFragment"
            app:destination="@id/propertyDetailsFragment" />
    </fragment>

    <fragment
        android:id="@+id/newPropertyFragment"
        android:name="el.dv.dvproperties.feature.newproperty.view.NewPropertyFragment"
        android:label="NewPropertyFragment" />

    <fragment
        android:id="@+id/propertyDetailsFragment"
        android:name="el.dv.dvproperties.feature.propertydetails.view.PropertyDetailsFragment"
        android:label="PropertyDetailsFragment" />
</navigation>
